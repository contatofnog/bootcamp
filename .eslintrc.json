{
  "env": {
      "es6": true,
      "node": true
  },
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "extends": ["plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "complexity": ["warn", 10],
    "no-unused-vars": ["warn", {"argsIgnorePattern": "^_"}]
  }
}
